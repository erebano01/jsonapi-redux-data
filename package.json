{
	"name": "jsonapi-redux-data",
	"version": "1.0.17",
	"description": "Library that makes integration of jsonapi with redux effortless and easy",
	"repository": {
		"type": "git",
		"url": "git://github.com/wednesday-solutions/jsonapi-redux-data.git"
	},
	"engines": {
		"npm": ">=5",
		"node": ">=8.15.1"
	},
	"keywords": [
		"jsonapi",
		"redux",
		"json-api",
		"redux-data",
		"reactjs",
		"react",
		"api",
		"apisauce"
	],
	"author": "Mac",
	"homepage": "https://github.com/wednesday-solutions/jsonapi-redux-data",
	"license": "MIT",
	"main": "dist/jsonapi-redux-data",
	"scripts": {
		"lint": "npm run lint:js",
		"lint:eslint": "eslint --ignore-path .eslintignore",
		"lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
		"lint:js": "npm run lint:eslint -- . ",
		"lint:staged": "lint-staged",
		"test:clean": "rimraf ./coverage",
		"test": "NODE_ENV=test && jest --coverage",
		"test:staged": "jest --findRelatedTests",
		"test:watch": "export NODE_ENV=test && jest --watchAll",
		"coveralls": "cat ./coverage/lcov.info | coveralls",
		"prettify": "prettier --write",
		"webpack:prod": "webpack -p --mode=production",
		"prepublishOnly": "yarn webpack:prod"
	},
	"browserslist": [
		"last 2 versions",
		"> 1%",
		"IE 10"
	],
	"lint-staged": {
		"*.js": [
			"npm run lint:eslint:fix",
			"git add --force",
			"jest --findRelatedTests $STAGED_FILES"
		],
		"*.json": [
			"prettier --write",
			"git add --force"
		]
	},
	"pre-commit": "lint:staged",
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"dependencies": {
		"apisauce": "^1.1.0",
		"deepmerge": "^4.2.2",
		"lodash": "4.17.11",
		"pluralize": "^8.0.0"
	},
	"peerDependencies": {
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"redux": "4.0.1"
	},
	"devDependencies": {
		"@babel/core": "7.4.3",
		"babel-eslint": "^10.1.0",
		"babel-loader": "8.0.5",
		"babel-plugin-lodash": "^3.3.4",
		"coveralls": "3.0.3",
		"eslint": "5.16.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest-cli": "24.7.1",
		"lint-staged": "8.1.5",
		"lodash-webpack-plugin": "^0.11.5",
		"pre-commit": "^1.2.2",
		"prettier": "1.17.0",
		"rimraf": "2.6.3",
		"terser-webpack-plugin": "^4.1.0",
		"webpack": "^4.44.1",
		"webpack-bundle-analyzer": "^3.8.0",
		"webpack-cli": "^3.3.12"
	}
}
